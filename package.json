{
  "name": "notes-app-api",
  "version": "1.1.0",
  "description": "A Node.js starter for the Serverless Framework with async/await and unit test support",
  "main": "handler.js",
  "scripts": {
    "localCreate": "serverless invoke local --function create --path mocks/create-event.json",
    "localGet": "serverless invoke local --function get --path mocks/get-event.json",
    "deploy": "serverless deploy -v",
    "deploy:prod": "serverless deploy -v --stage prod",
    "test": "serverless-bundle test"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-nodejs-starter.git"
  },
  "devDependencies": {
    "aws-sdk": "^2.533.0",
    "serverless-bundle": "^1.2.2",
    "serverless-offline": "^5.3.3"
  },
  "dependencies": {
    "uuid": "^3.3.3"
  }
}
